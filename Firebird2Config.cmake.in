@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Include client library targets if available (not built when FIREBIRD_BUILD_FBCLIENT=OFF)
if(EXISTS "${CMAKE_CURRENT_LIST_DIR}/Firebird2Targets.cmake")
    include("${CMAKE_CURRENT_LIST_DIR}/Firebird2Targets.cmake")
endif()

# Include embedded library targets if available
if(EXISTS "${CMAKE_CURRENT_LIST_DIR}/Firebird2EmbedTargets.cmake")
    include("${CMAKE_CURRENT_LIST_DIR}/Firebird2EmbedTargets.cmake")
endif()

# Check for required components
check_required_components(Firebird2)
